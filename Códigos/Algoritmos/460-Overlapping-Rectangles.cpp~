//460 - Overlapping Rectangles
#include <algorithm>
#include <stdio.h>

using namespace std;

typedef struct Punto{
	int x, y;
}PUN;

typedef struct Rectangulo{
	PUN a, b;
}REC;

bool inRec(PUN p, REC r){ 
	if( p.x <= r.b.x && p.x >= r.a.x && p.y >= r.a.y && p.y <= r.b.y)
		return true;
	return false;
}

int main()
{
	return 0;
}
